public service Tracking::HeartRateConstants.initialize () is
hrc: instance of HeartRateConstants;
begin
  hrc := find_one HeartRateConstants();
  if ( null = hrc ) then
    hrc := create HeartRateConstants( id => 1 );
    hrc.HeartRateAveragingWindow := 5;
    hrc.HeartRateSamplingPeriod := 3;
  end if;
end service;
