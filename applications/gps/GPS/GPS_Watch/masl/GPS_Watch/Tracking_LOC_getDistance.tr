public service Tracking::LOC~>getDistance ( result: out real,
                                            toLong: in real,
                                            toLat: in real,
                                            fromLong: in real,
                                            fromLat: in real ) is
distance: real;
begin
  Location::getDistance( distance, toLong, toLat, fromLong, fromLat );
  result := distance;
end service;
